{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/url",
      "dest": "/server.js"
    },
    {
      "src": "/:short_id",
      "dest": "/server.js"
    },
    {
      "src": "/:/:custom_url",
      "dest": "/server.js"
    }
  ]
}
