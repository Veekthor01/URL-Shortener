{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/:short_id",
        "dest": "/server.js"
      },
      {
        "src": "/c/:custom_url",
        "dest": "/server.js"
      }
      ]
  }